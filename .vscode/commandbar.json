    {
      "text": "Git:clean up local branches",
      "color": "white",
      "commandType": "exec",
      "command": "git for-each-ref --format '%(refname:short)' refs/heads | grep -v \"master\\|main\\|develop\\|stage\" | grep -v $(git branch --show-current) | xargs git branch -D",
