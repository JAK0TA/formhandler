    </sEMAILUSER>
    <sMISC>
      <ROOT>
        <sheetTitle>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:sheet_miscSettings</sheetTitle>
        <type>array</type>
        <el>
          <redirect_page>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:redirectPage</label>
            <config>
              <type>group</type>
              <internal_type>db</internal_type>
              <allowed>pages</allowed>
              <size>1</size>
              <maxitems>1</maxitems>
              <minitems>0</minitems>
              <show_thumbs>1</show_thumbs>
              <wizards>
                <suggest>
                  <type>suggest</type>
                </suggest>
              </wizards>
            </config>
          </redirect_page>

          <required_fields>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:mandatoryFields</label>
            <config>
              <type>input</type>
              <eval>text</eval>
              <size>500</size>
              <placeholder>firstname, lastname, email</placeholder>
            </config>
          </required_fields>
        </el>
      </ROOT>
    </sMISC>
  </sheets>
</T3DataStructure>
