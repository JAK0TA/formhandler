<T3DataStructure>
  <sheets>
    <sDEF>
      <ROOT>
        <sheetTitle>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:sheet_general</sheetTitle>
        <type>array</type>
        <el>
          <template_file>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:template_file</label>
            <config>
              <type>group</type>
              <internal_type>db</internal_type>
              <allowed>sys_file</allowed>
              <max_size>500</max_size>
              <minitems>0</minitems>
              <maxitems>1</maxitems>
              <size>1</size>
              <selectedListStyle>Width:180px</selectedListStyle>
              <appearance>
                <elementBrowserType>file</elementBrowserType>
                <elementBrowserAllowed>tmpl,html,htm</elementBrowserAllowed>
              </appearance>
              <eval>trim</eval>
            </config>
          </template_file>
          <lang_file>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:lang_file</label>
            <config>
              <type>group</type>
              <internal_type>db</internal_type>
              <allowed>sys_file</allowed>
              <max_size>500</max_size>
              <minitems>0</minitems>
              <maxitems>1</maxitems>
              <size>1</size>
              <appearance>
                <elementBrowserType>file</elementBrowserType>
                <elementBrowserAllowed>xlf</elementBrowserAllowed>
              </appearance>
              <eval>trim</eval>
            </config>
          </lang_file>
          <predefined>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:template_predefined</label>
            <config>
              <type>select</type>
              <renderType>selectSingle</renderType>
              <itemsProcFunc>Typoheads\Formhandler\TcaFormElement\PredefinedForm->addItems</itemsProcFunc>
              <maxitems>1</maxitems>
              <size>1</size>
              <multiple>0</multiple>
            </config>
          </predefined>
        </el>
      </ROOT>
    </sDEF>
    <sEMAILADMIN>
