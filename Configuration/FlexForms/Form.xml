              <size>50</size>
            </config>
          </replyto_name>

          <cc_email>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:cc</label>
            <config>
              <type>input</type>
              <eval>text</eval>
              <size>50</size>
            </config>
          </cc_email>

          <cc_name>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:ccName</label>
            <config>
              <type>input</type>
              <eval>text</eval>
              <size>50</size>
            </config>
          </cc_name>

          <bcc_email>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:bcc</label>
            <config>
              <type>input</type>
              <eval>text</eval>
              <size>50</size>
            </config>
          </bcc_email>

          <bcc_name>
            <label>LLL:EXT:formhandler/Resources/Private/Language/locallang_flexform.xlf:bccName</label>
            <config>
              <type>input</type>
              <eval>text</eval>
              <size>50</size>
            </config>
          </bcc_name>
        </el>
      </ROOT>
    </sEMAILADMIN>
